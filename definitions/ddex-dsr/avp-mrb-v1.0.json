        { "position": 5, "name": "dsp_resource_id", "type": "string", "required": true },
        { "position": 6, "name": "usages", "type": "integer", "required": true },
        { "position": 7, "name": "net_revenue", "type": "decimal", "typeOpt": { "scale": 8 }, "required": true },
