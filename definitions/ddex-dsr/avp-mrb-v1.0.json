{
  "id": "ddex-dsr-avp-mrb-1.0",
  "formatType": "ddex-dsr-avp-mrb",
  "version": "1.0",
  "description": "DDEX DSR AVP (Audio Visual Profile) MRB (Multiple Record Block Variant) Version 1.0",
  "models": {
    "HEAD": {
      "description": "Header",
      "fields": [
        { "position": 1, "name": "record_type", "type": "string", "required": true, "allowedValues": ["HEAD"] },
        {
          "position": 2,
          "name": "message_version",
          "type": "string",
          "required": true,
          "allowedPattern": "dsrf(\\/30|(\\/\\\\d+(\\.\\\\d+){0,2}){3})"
        },
        {
          "position": 3,
          "name": "profile",
          "type": "string",
          "required": true,
          "allowedPattern": "Audio\\\\s*Visual\\\\s*Profile"
        },
        { "position": 4, "name": "profile_version", "type": "string", "required": true, "allowedValues": ["1.0"] },
        { "position": 5, "name": "message_id", "type": "string", "required": true },
        {
          "position": 6,
          "name": "message_created_date_time",
          "type": "datetime",
          "required": true,
          "allowedValues": ["YYYY-MM-DDTHH24:MI:SSZ"]
        },
        { "position": 7, "name": "file_number", "type": "integer", "required": true },
        { "position": 8, "name": "number_of_files", "type": "integer", "required": true },
        {
          "position": 9,
          "name": "usage_start_date",
          "type": "date",
          "required": true,
          "allowedValues": ["YYYY-MM-DD", "YYYY-MM", "YYYY"]
        },
        {
          "position": 10,
          "name": "usage_end_date",
          "type": "date",
          "required": true,
          "allowedValues": ["YYYY-MM-DD", "YYYY-MM", "YYYY"]
        },
        {
          "position": 11,
          "name": "sender_party_id",
          "type": "string",
          "required": true,
          "allowedPattern": "PADPIDA[0-9A-Za-z]{11}"
        },
        { "position": 12, "name": "sender_name", "type": "string", "required": true },
        { "position": 13, "name": "service_description", "type": "string", "required": false },
        {
          "position": 14,
          "name": "recipient_party_id",
          "type": "string",
          "required": false,
          "allowedPattern": "PADPIDA[0-9A-Za-z]{11}"
        },
        { "position": 15, "name": "recipient_name", "type": "string", "required": false },
        { "position": 16, "name": "represented_repertoire", "type": "array", "required": false }
      ]
    },
    "FOOT": {
      "description": "Footer",
      "fields": [
        { "position": 1, "name": "record_type", "type": "string", "required": true, "allowedValues": ["FOOT"] },
        { "position": 2, "name": "number_of_lines_in_file", "type": "integer", "required": true },
        { "position": 3, "name": "number_of_lines_in_report", "type": "integer", "required": false },
        { "position": 4, "name": "number_of_summary_records", "type": "integer", "required": true },
        { "position": 5, "name": "number_of_blocks_in_file", "type": "integer", "required": true },
        { "position": 6, "name": "number_of_blocks_in_report", "type": "integer", "required": false }
      ]
    },
    "SY04.01": {
      "description": "Per-subscriber Minima Summary Record for Subscription Services",
      "fields": [
        { "position": 1, "name": "record_type", "type": "string", "required": true, "allowedValues": ["SY04.01"] },
        { "position": 2, "name": "summary_record_id", "type": "string", "required": true },
        { "position": 3, "name": "distribution_channel", "type": "string", "required": false },
        {
          "position": 4,
          "name": "distribution_channel_dpid",
          "type": "string",
          "required": false,
          "allowedPattern": "PADPIDA[0-9A-Za-z]{11}"
        },
        { "position": 5, "name": "commercial_model", "type": "enum", "typeRef": "commercial_model", "required": true },
        { "position": 6, "name": "use_type", "type": "enum", "typeRef": "use_type", "required": false },
        { "position": 7, "name": "territory", "type": "enum", "typeRef": "territory", "required": true },
        { "position": 8, "name": "service_description", "type": "string", "required": true },
        { "position": 9, "name": "subscriber_type", "type": "string", "required": true },
        { "position": 10, "name": "subscribers", "type": "decimal", "typeOpt": { "scale": 8 }, "required": true },
        {
          "position": 11,
          "name": "sub_period_start_date",
          "type": "date",
          "required": false,
          "allowedValues": ["YYYY-MM-DD", "YYYY-MM", "YYYY"]
        },
        {
          "position": 12,
          "name": "sub_period_end_date",
          "type": "date",
          "required": false,
          "allowedValues": ["YYYY-MM-DD", "YYYY-MM", "YYYY"]
        },
        { "position": 13, "name": "usages_in_sub_period", "type": "integer", "required": false },
        { "position": 14, "name": "usages_in_reporting_period", "type": "integer", "required": false },
        { "position": 15, "name": "currency_of_reporting", "type": "enum", "typeRef": "currency", "required": true },
        { "position": 16, "name": "currency_of_transaction", "type": "enum", "typeRef": "currency", "required": false },
        { "position": 17, "name": "exchange_rate", "type": "decimal", "typeOpt": { "scale": 8 }, "required": false },
        {
          "position": 18,
          "name": "consumer_paid_unit_price",
          "type": "decimal",
          "typeOpt": { "scale": 8 },
          "required": true
        },
        { "position": 19, "name": "net_revenue", "type": "decimal", "typeOpt": { "scale": 8 }, "required": true },
        {
          "position": 20,
          "name": "music_usage_percentage",
          "type": "decimal",
          "typeOpt": { "scale": 8 },
          "required": true,
          "minSize": 0,
          "maxSize": 100
        }
      ]
    },
    "RE03": {
      "description": "Audio-visual Release Record",
      "fields": [
        { "position": 1, "name": "record_type", "type": "string", "required": true, "allowedValues": ["RE03"] },
        { "position": 2, "name": "block_id", "type": "string", "required": true },
        { "position": 3, "name": "release_reference", "type": "string", "required": true },
        { "position": 4, "name": "dsp_release_id", "type": "string", "required": true },
        {
          "position": 5,
          "name": "proprietary_release_id",
          "type": "array",
          "items": { "type": "string", "allowedPattern": "\\.+::\\.+" },
          "required": false
        },
        { "position": 6, "name": "icpn", "type": "string", "required": false },
        { "position": 7, "name": "title", "type": "string", "required": true },
        { "position": 8, "name": "sub_title", "type": "string", "required": false },
        { "position": 9, "name": "series_title", "type": "string", "required": false },
        { "position": 10, "name": "season_number", "type": "integer", "required": false },
        { "position": 11, "name": "display_artist_name", "type": "string", "required": false },
        {
          "position": 12,
          "name": "display_artist_party_id",
          "type": "string",
          "required": false,
          "allowedPattern": "INSI::\\\\d{16}"
        },
        { "position": 13, "name": "release_type", "type": "enum", "typeRef": "release_type", "required": false },
        { "position": 14, "name": "data_provider_name", "type": "string", "required": false }
      ]
    },
    "AS03": {
      "description": "Audio-visual Resource Record",
      "fields": [
        { "position": 1, "name": "record_type", "type": "string", "required": true, "allowedValues": ["AS03"] },
        { "position": 2, "name": "block_id", "type": "string", "required": true },
        { "position": 3, "name": "resource_reference", "type": "string", "required": true },
        { "position": 4, "name": "dsp_resource_id", "type": "string", "required": true },
        {
          "position": 5,
          "name": "isan",
          "type": "string",
          "required": false,
          "allowedPattern": "([0-9A-Fa-f]{4}-?){3}([0-9A-Fa-f]{4}(-?[\\\\w\\\\d])?(-?([0-9A-Fa-f]{4}-?){2}[\\\\w\\\\d]?)?)"
        },
        {
          "position": 6,
          "name": "eidr",
          "type": "string",
          "required": false,
          "allowedPattern": "\\\\d{2}\\.\\\\d{4}\\/([0-9A-Fa-f]{4}-?){5}\\\\w"
        },
        {
          "position": 7,
          "name": "proprietary_id",
          "type": "string",
          "required": false,
          "allowedPattern": "\\.+::\\.+"
        },
        { "position": 8, "name": "video_type", "type": "enum", "typeRef": "video_type", "required": true },
        { "position": 9, "name": "title", "type": "string", "required": true },
        { "position": 10, "name": "sub_title", "type": "string", "required": false },
        { "position": 11, "name": "original_title", "type": "string", "required": false },
        { "position": 12, "name": "season_number", "type": "integer", "required": false },
        { "position": 13, "name": "episode_number", "type": "integer", "required": false },
        { "position": 14, "name": "genre", "type": "string", "required": false },
        {
          "position": 15,
          "name": "duration",
          "type": "duration",
          "required": true,
          "allowedPattern": "PT((\\\\d+H)?\\\\d+M)?\\\\d+(\\.\\\\d+)?S"
        },
        { "position": 16, "name": "producer_name", "type": "array", "items": { "type": "string" }, "required": false },
        {
          "position": 17,
          "name": "producer_party_id",
          "type": "array",
          "items": { "type": "string", "allowedPattern": "INSI::\\\\d{16}" },
          "required": false
        },
        { "position": 18, "name": "director_name", "type": "array", "items": { "type": "string" }, "required": false },
        {
          "position": 19,
          "name": "director_party_id",
          "type": "array",
          "items": { "type": "string", "allowedPattern": "INSI::\\\\d{16}" },
          "required": false
        },
        { "position": 20, "name": "actor_name", "type": "array", "items": { "type": "string" }, "required": false },
        {
          "position": 21,
          "name": "actor_party_id",
          "type": "array",
          "items": { "type": "string", "allowedPattern": "INSI::\\\\d{16}" },
          "required": false
        },
        {
          "position": 22,
          "name": "language_localization_type",
          "type": "enum",
          "typeRef": "language",
          "required": false
        },
        { "position": 23, "name": "has_captioning", "type": "boolean", "required": false },
        { "position": 24, "name": "has_audio_description", "type": "boolean", "required": false },
        { "position": 25, "name": "language_of_performance", "type": "enum", "typeRef": "language", "required": false },
        { "position": 26, "name": "language_of_dubbing", "type": "enum", "typeRef": "language", "required": false },
        {
          "position": 27,
          "name": "production_or_release_date",
          "type": "date",
          "required": false,
          "allowedValues": ["YYYY-MM-DD", "YYYY-MM", "YYYY"]
        },
        {
          "position": 28,
          "name": "country_of_production",
          "type": "array",
          "items": { "type": "enum", "typeRef": "territory" },
          "required": false
        }
      ]
    },
    "SU03.01": {
      "description": "UGC (User Generated Content) Sales and Usage Record",
      "fields": [
        { "position": 1, "name": "record_type", "type": "string", "required": true, "allowedValues": ["SU03.01"] },
